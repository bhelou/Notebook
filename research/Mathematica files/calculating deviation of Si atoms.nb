Cell["\<\
Try to validate the heat capacity\
\>", "Text",
 CellChangeTimes->{{3.5575266765356293`*^9, 3.557526682180768*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"avgEnergyAtom", "[", 
    RowBox[{"temperature_", ",", "C11_", ",", " ", "IncludeGroundState_"}], 
    "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "Kec", ",", " ", "\[Xi]", ",", "constantFactor", ",", "integrand", ",", 
       " ", "avgEnergy"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Kec", "=", 
       RowBox[{"a", "*", "C11"}]}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Spring", " ", "constant"}], ",", " ", 
        RowBox[{
        "we", " ", "assume", " ", "a", " ", "potential", " ", "of", " ", 
         FractionBox["1", "2"], 
         SuperscriptBox["kx", "2"]}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"integrand", " ", "=", " ", 
       RowBox[{
        RowBox[{"y", "/", 
         RowBox[{"Sqrt", "[", 
          RowBox[{
           RowBox[{"\[Xi]", "^", "2"}], "-", 
           RowBox[{"y", "^", "2"}]}], "]"}]}], "*", 
        RowBox[{"(", 
         RowBox[{"1", "/", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Exp", "[", "y", "]"}], "-", "1"}], ")"}]}], ")"}]}]}], 
      ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"\[Xi]", "=", 
       RowBox[{"2", "*", "\[Beta]", "*", "\[HBar]", "*", 
        RowBox[{"Sqrt", "[", 
         RowBox[{"Kec", "/", "M"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"constantFactor", " ", "=", " ", 
       RowBox[{
        RowBox[{"2", "/", "Pi"}], "/", "\[Beta]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"avgEnergy", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"constantFactor", "/.", 
          RowBox[{"{", 
           RowBox[{"T", "\[Rule]", " ", "temperature"}], "}"}]}], ")"}], "*", 
        RowBox[{"NIntegrate", "[", 
         RowBox[{
          RowBox[{"integrand", "/.", 
           RowBox[{"{", 
            RowBox[{"T", "\[Rule]", " ", "temperature"}], "}"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"y", ",", "0", ",", 
            RowBox[{"(", 
             RowBox[{"\[Xi]", "/.", 
              RowBox[{"{", 
               RowBox[{"T", "\[Rule]", " ", "temperature"}], "}"}]}], ")"}]}],
            "}"}], ",", "\[IndentingNewLine]", 
          RowBox[{"MaxRecursion", "\[Rule]", " ", "13"}]}], "]"}]}]}], ";", 
      " ", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "The", " ", "maxrecursion", " ", "is", " ", "needed", " ", "because", 
         " ", "as", " ", "y", " ", "approaches", " ", "\[Xi]"}], ",", " ", 
        RowBox[{
        "the", " ", "integrand", " ", "becomes", " ", "very", " ", 
         "small"}]}], "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"IncludeGroundState", " ", "\[Equal]", " ", "True"}], ",", 
        " ", "\[IndentingNewLine]", 
        RowBox[{"avgEnergy", " ", "=", " ", 
         RowBox[{
          RowBox[{"avgEnergy", "+", "groundEng"}], "/.", 
          RowBox[{"{", " ", 
           RowBox[{"Kconstant", "\[Rule]", " ", "Kec"}], "}"}]}]}]}], "]"}], 
      ";", "\[IndentingNewLine]", "\[IndentingNewLine]", "avgEnergy"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.5574183203073*^9, 3.557418337915556*^9}}],

