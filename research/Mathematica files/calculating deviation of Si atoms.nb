Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Xi]", "=", 
   RowBox[{"2", "*", "\[Beta]", "*", "\[HBar]", "*", 
    RowBox[{"Sqrt", "[", 
     RowBox[{"Kconstant", "/", "M"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"groundEng", "=", 
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"T", ">", "0"}], ",", " ", 
       RowBox[{"Kconstant", ">", "0"}]}], "}"}], ",", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"y", "/", 
         RowBox[{"Sqrt", "[", 
          RowBox[{
           SuperscriptBox["\[Xi]", "2"], "-", 
           RowBox[{"y", "^", "2"}]}], "]"}]}], "*", 
        RowBox[{"1", "/", "2"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"y", ",", "0", ",", "\[Xi]"}], "}"}]}], "]"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"groundEng", " ", "=", " ", 
   RowBox[{"groundEng", "*", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", "/", "\[Pi]"}], "/", "\[Beta]"}], ")"}]}]}], 
  ";"}]}], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.5573534688413506`*^9, 3.5573535033423643`*^9}, {
  3.5573535371363635`*^9, 3.557353590867029*^9}, {3.557354151568474*^9, 
  3.55735416012875*^9}, {3.55735419375473*^9, 3.557354225935681*^9}, {
