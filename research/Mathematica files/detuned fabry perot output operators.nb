   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fvec2", " ", "=", " ", 
     RowBox[{"Series", "[", 
      RowBox[{
       RowBox[{"Fvec2", "/.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", "\[Rule]", " ", 
           RowBox[{"x", "*", "\[Epsilon]"}]}], ",", 
          RowBox[{"T", "\[Rule]", " ", 
           RowBox[{"T", "*", "\[Epsilon]"}]}]}], "}"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"\[Epsilon]", ",", "0", ",", 
         RowBox[{"-", "1"}]}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fvec2", " ", "=", " ", 
     RowBox[{"Simplify", "[", 
      RowBox[{"Normal", "[", "Fvec2", "]"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fvec2", " ", "=", " ", 
     RowBox[{"Simplify", "[", 
      RowBox[{"Fvec2", "/.", 
       RowBox[{"{", 
        RowBox[{"\[Epsilon]", "\[Rule]", " ", "1"}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"absArgForm", "[", 
      RowBox[{"Fvec2", ",", "False"}], "]"}], ")"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}]}]], "Input",
