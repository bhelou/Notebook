Cell[BoxData[
 FractionBox[
  RowBox[{"2", " ", 
   SqrtBox["2"], " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"\[ImaginaryI]", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "\[Pi]"}], "+", 
       RowBox[{"ArcTan", "[", 
        FractionBox[
         RowBox[{"4", " ", "x"}], "T"], "]"}]}], ")"}]}]], " ", 
   SqrtBox[
    FractionBox["\[Kappa]", 
     RowBox[{
      SuperscriptBox["T", "2"], "+", 
      RowBox[{"16", " ", 
       SuperscriptBox["x", "2"]}]}]]]}], "gSQL"]], "Output",
