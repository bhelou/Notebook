Cell[TextData[{
 "We remember the definition  \[Phi]\[Congruent]",
 Cell[BoxData[
  FormBox[
   RowBox[{"ArcTan", "[", 
    FractionBox[
     RowBox[{"4", "x"}], "T"], "]"}], TraditionalForm]],
  CellFrameColor->GrayLevel[0]],
 " (",
 ButtonBox["result1 H22",
  BaseStyle->"Hyperlink",
  ButtonData->"result1 H22"],
 "). Further, \n",
 Cell[BoxData[
  FormBox[GridBox[{
     {GridBox[{
        {
         RowBox[{
          RowBox[{
           RowBox[{"-", 
            SubscriptBox["g", "SQL"]}], "\[Times]", 
           SubscriptBox["F", "2"]}], "=", 
          RowBox[{"2", " ", 
           SqrtBox["2"], " ", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"\[ImaginaryI]\[Phi]", " "}]], 
           SqrtBox[
            FractionBox["\[Kappa]", 
             RowBox[{
              SuperscriptBox["T", "2"], "+", 
              RowBox[{"16", " ", 
               SuperscriptBox["x", "2"]}]}]]]}]}]},
        {
         RowBox[{"=", 
          RowBox[{"2", " ", 
           SqrtBox["2"], " ", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"\[ImaginaryI]\[Phi]", " "}]], 
           SqrtBox[
            RowBox[{
             FractionBox["1", 
              RowBox[{"4", "T"}]], 
             FractionBox[
              RowBox[{"4", "T\[Kappa]"}], 
              RowBox[{
               SuperscriptBox["T", "2"], "+", 
               RowBox[{"16", " ", 
                SuperscriptBox["x", "2"]}]}]]}]]}]}]},
        {
         RowBox[{"=", 
          RowBox[{"2", " ", 
           SqrtBox["2"], " ", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"\[ImaginaryI]\[Phi]", " "}]], 
           SqrtBox[
            RowBox[{
             FractionBox["1", 
              RowBox[{"4", "T"}]], 
             FractionBox["T", "8"], 
             FractionBox[
              RowBox[{"  ", 
               RowBox[{"2", "\[Gamma]", "\[Times]", 
                SubscriptBox["\[Iota]", "c"], " "}]}], 
              RowBox[{
               SuperscriptBox["\[CapitalOmega]", "2"], "(", 
               RowBox[{
                SuperscriptBox["\[Gamma]", "2"], "+", 
                SuperscriptBox["\[CapitalOmega]", "2"]}], ")"}]]}]]}]}]},
        {
         RowBox[{"=", " ", 
          RowBox[{
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"\[ImaginaryI]\[Phi]", " "}]], 
           FractionBox["1", "2"], 
           SqrtBox[
            FractionBox[
             RowBox[{"  ", 
              RowBox[{"2", "\[Gamma]", "\[Times]", 
               SubscriptBox["\[Iota]", "c"], " "}]}], 
             RowBox[{
              SuperscriptBox["\[CapitalOmega]", "2"], "(", 
              RowBox[{
               SuperscriptBox["\[Gamma]", "2"], "+", 
               SuperscriptBox["\[CapitalOmega]", "2"]}], ")"}]]]}]}]}
       },
       GridBoxAlignment->{"Columns" -> {{"="}}}]}
    },
    GridBoxItemSize->{"Columns" -> {{
         Scaled[0.96]}}}], TraditionalForm]]],
 "\nwhere we used ",
 ButtonBox["result1 H22",
  BaseStyle->"Hyperlink",
  ButtonData->"result1 H22"],
 " in the third equality."
}], "Text",
