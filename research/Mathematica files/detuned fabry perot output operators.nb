Cell[TextData[{
 "Hence, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "2"], "="}], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]\[Phi]", " "}]]}], 
    FractionBox["1", 
