\begin_layout Section
Dirac delta function 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\delta\left(x-a\right) & = & \frac{1}{2\pi}\intop_{-\infty}^{\infty}\exp\left[i\left(x-a\right)k\right]dk\\
\delta\left(x-a\right) & = & \frac{1}{2\pi}\intop_{-\infty}^{\infty}\exp\left[-i\left(x-a\right)k\right]dk
\end{eqnarray*}

\end_inset

(From Wikipedia) 
\end_layout

\begin_layout Standard
We can also write that 
\begin_inset Formula 
\begin{eqnarray}
\delta\left(t-t_{0}\right) & = & \lim_{\Delta t\rightarrow0}\frac{\delta_{t\in\left[t_{0}-\Delta t/2,t_{0}+\Delta t/2\right]}}{\Delta t}\label{eq:physics delta function}\\
\nonumber 
\end{eqnarray}

\end_inset

*
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
Integrating 
\begin_inset Formula 
\begin{eqnarray*}
\int_{-\infty}^{\infty}dt\lim_{\Delta t\rightarrow0}\frac{\delta_{t\in\left[t_{0}-\Delta t/2,t_{0}+\Delta t/2\right]}}{\Delta t} & = & \int_{-\infty}^{\infty}dt\frac{\delta_{t\in\left[t_{0}-dt/2,t_{0}+dt/2\right]}}{dt}\\
 & = & \int_{t_{0}-dt/2}^{t_{0}+dt/2}dt\frac{1}{dt}=\frac{1}{dt}\times dt\\
 & = & 1
\end{eqnarray*}

\end_inset

Also, 
\begin_inset Formula 
\begin{eqnarray*}
\int_{-\infty}^{\infty}dtf\left(t\right)\lim_{\Delta t\rightarrow0}\frac{\delta_{t\in\left[t_{0}-\Delta t/2,t_{0}+\Delta t/2\right]}}{\Delta t} & = & \int_{-\infty}^{\infty}dt\frac{\delta_{t\in\left[t_{0}-dt/2,t_{0}+dt/2\right]}}{dt}f\left(t\right)\\
 & = & \int_{t_{0}-dt/2}^{t_{0}+dt/2}dt\frac{f\left(t\right)}{dt}=\frac{F\left(t_{0}+dt/2\right)-F\left(t_{0}-dt/2\right)}{dt}\\
 & = & f\left(t_{0}\right).
\end{eqnarray*}

\end_inset


\end_layout

\end_inset


\end_layout

