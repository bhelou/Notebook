  FormBox[GridBox[{
     {GridBox[{
        {
         RowBox[{
          RowBox[{
           RowBox[{"-", 
            SubscriptBox["g", "SQL"]}], "\[Times]", 
           SubscriptBox["F", "2"]}], "=", 
          RowBox[{"2", " ", 
