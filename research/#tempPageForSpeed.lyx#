\begin_layout Subsubsection
LIGO like setup and small detuned phase
\end_layout

\begin_layout Standard
An advanced LIGO-like setup would also satisfy the following criteria
\begin_inset CommandInset citation
LatexCommand cite
key "advancedLIGOdata"

\end_inset

:
\end_layout

\begin_layout Criterion
Both 
\begin_inset Formula $\Omega\tau$
\end_inset

 and 
\begin_inset Formula $T$
\end_inset

 are small and are of approximately the same order - e.g.
 for LIGO they are both ~
\begin_inset Formula $10^{-3}$
\end_inset

*
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
See 
\begin_inset CommandInset ref
LatexCommand ref
reference "note3"

\end_inset


\end_layout

\end_inset

.
\end_layout

\begin_layout Standard
We also make the following assumption
\end_layout

\begin_layout Assumption
The detuned phase is small and on the same order of magnitude as 
\begin_inset Formula $\Omega\tau$
\end_inset

 and 
\begin_inset Formula $T$
\end_inset

.
\end_layout

\begin_layout Standard
where
\begin_inset Formula 
\begin{eqnarray*}
\phi & \equiv & \tan^{-1}\left(\Omega/\gamma\right)\\
\gamma & \equiv & \frac{Tc}{4L}=\frac{T}{4\tau}\\
\mathcal{K} & \equiv & \frac{2\gamma\iota_{c}}{\Omega^{2}\left(\Omega^{2}+\gamma^{2}\right)}\quad\\
\iota_{c} & \equiv & \frac{8\omega_{0}I_{c}}{mLc}=\frac{4}{T}\frac{8\omega_{0}I_{0}}{mLc}=\frac{4}{T}\frac{c\Omega^{2}}{L}\kappa\\
 & = & \frac{4}{T}\frac{\Omega^{2}}{\tau}\kappa
\end{eqnarray*}

\end_inset

We assume that we are working close enough to resonance that 
\begin_inset Formula $I_{c}=4I_{0}/T$
\end_inset

.
\end_layout

\begin_layout Standard
Hence,
\begin_inset Formula 
\begin{equation}
\kappa=\frac{T\tau}{4\Omega^{2}}\iota_{c}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
I_{c}\equiv\frac{4I_{0}}{T}.
\end{equation}

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\kappa & = & \frac{8I_{0}\omega_{0}}{mc^{2}\Omega^{2}}\\
g_{SQL} & = & \sqrt{2\hbar m\Omega^{2}}.
\end{eqnarray*}

\end_inset

We have 
\begin_inset Formula 
\begin{eqnarray}
\xi & \equiv & \frac{2T^{2}\theta}{-\frac{1}{4}T(T-4ix)^{2}-4T\theta^{2}-4(-2+T-4ix)\theta\kappa}\\
 & = & \frac{2T^{2}\theta}{-\frac{1}{4}T(T-4i\Omega\tau)^{2}-4T\theta^{2}-4(-2+T-4\Omega\tau i)\theta\frac{T\tau}{4\Omega^{2}}\iota_{c}}\\
 & = & \frac{2T^{2}\Delta/\tau}{-\frac{1}{4}T(T/\tau-4i\Omega)^{2}-4T\Delta^{2}-4(-2+T-4\Omega\tau i)\Delta\frac{T}{4\Omega^{2}}\iota_{c}}\\
\mbox{} & = & \frac{8T\Delta\gamma}{-\frac{1}{4}T(T/\tau-4i\Omega)^{2}-4T\Delta^{2}-4(-2+T-4\Omega\tau i)\Delta\frac{T}{4\Omega^{2}}\iota_{c}}\\
 & = & \frac{8\Delta\gamma}{-\frac{1}{4}(T/\tau-4i\Omega)^{2}-4\Delta^{2}-4(-2+T-4\Omega\tau i)\Delta\frac{1}{4\Omega^{2}}\iota_{c}}\\
 & = & \frac{8\Delta\gamma\Omega^{2}}{\Omega^{2}\left[-\frac{1}{4}(T/\tau-4i\Omega)^{2}-4\Delta^{2}\right]-(-2+T-4\Omega\tau i)\Delta\iota_{c}}\\
 & = & \frac{2\Delta\gamma\Omega^{2}}{\Omega^{2}\left[-\frac{1}{16}(T/\tau-4i\Omega)^{2}-\Delta^{2}\right]-(-2+T-4\Omega\tau i)\Delta\iota_{c}/4}\\
 & = & \frac{2\Delta\gamma\Omega^{2}}{\Omega^{2}\left[-(\gamma-i\Omega)^{2}-\Delta^{2}\right]+(1/2-T/4+\Omega\tau i)\Delta\iota_{c}}\\
 & \approx & \frac{2\Delta\gamma\Omega^{2}}{\Omega^{2}\left[(i\gamma+\Omega)^{2}-\Delta^{2}\right]+\Delta\iota_{c}/2}.
\end{eqnarray}

\end_inset

We have that
\begin_inset Formula 
\begin{eqnarray}
\xi_{dn} & = & -\frac{1}{4}T(T-4ix)^{2}-4T\theta^{2}-4(-2+T-4ix)\theta\kappa\\
 & = & \frac{1}{4}T(iT+4x)^{2}-4T\theta^{2}-4(-2+T-4ix)\theta\kappa\\
 & \approx & \frac{1}{4}T(iT+4x)^{2}-4T\theta^{2}+8\theta\kappa\\
 & = & \frac{1}{4}T(iT+4x)^{2}-4T\theta^{2}+8\theta\frac{T\tau}{4\Omega^{2}}\iota_{c}\\
 & = & \frac{\Omega^{2}T(iT+4x)^{2}-16T\Omega^{2}\theta^{2}+8\theta T\tau\iota_{c}}{4\Omega^{2}}\\
 & = & 4\Omega^{2}T(iT+4\Omega\tau)^{2}-16T\Omega^{2}\theta^{2}+8\theta T\tau\iota_{c}\\
 & = & \Omega^{2}T(i\gamma+\Omega)^{2}-\frac{T\Omega^{2}\theta^{2}}{\tau^{2}}+\frac{\theta T\iota_{c}}{2\tau}\\
 & = & \Omega^{2}T(i\gamma+\Omega)^{2}-T\Omega^{2}\Delta^{2}+\frac{\Delta T\iota_{c}}{2}\\
 & = & \Omega^{2}T\left[(i\gamma+\Omega)^{2}-\Delta^{2}\right]+\frac{\Delta T\iota_{c}}{2}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
We have that
\begin_inset Formula 
\begin{eqnarray}
\xi_{21} & = & \frac{2T(-T\theta+2\kappa+4ix\kappa)}{\Omega^{2}T\left[(i\gamma+\Omega)^{2}-\Delta^{2}\right]+\frac{\Delta T\iota_{c}}{2}}\\
 & = & \frac{2(-T\theta+2\kappa+4ix\kappa)}{\Omega^{2}\left[(i\gamma+\Omega)^{2}-\Delta^{2}\right]+\frac{\Delta\iota_{c}}{2}}\\
 & \approx & \frac{2(-T\theta+2\frac{T\tau}{4\Omega^{2}}\iota_{c})}{\Omega^{2}\left[(i\gamma+\Omega)^{2}-\Delta^{2}\right]+\frac{\Delta\iota_{c}}{2}}\\
 & = & \frac{2\tau(-T\Delta+2\frac{T}{4\Omega^{2}}\iota_{c})}{\Omega^{2}\left[(i\gamma+\Omega)^{2}-\Delta^{2}\right]+\frac{\Delta\iota_{c}}{2}}\\
 & = & \frac{2\tau^{2}(-\frac{T}{\tau}\Delta+2\frac{T}{4\tau\Omega^{2}}\iota_{c})}{\Omega^{2}\left[(i\gamma+\Omega)^{2}-\Delta^{2}\right]+\frac{\Delta\iota_{c}}{2}}\\
 & = & \frac{2\tau^{2}(-4\gamma\Delta+2\frac{\gamma}{\Omega^{2}}\iota_{c})}{\Omega^{2}\left[(i\gamma+\Omega)^{2}-\Delta^{2}\right]+\frac{\Delta\iota_{c}}{2}}\\
 & = & \frac{-4\gamma\Omega^{2}\Delta+2\gamma\iota_{c}}{\Omega^{2}\left[(i\gamma+\Omega)^{2}-\Delta^{2}\right]+\frac{\Delta\iota_{c}}{2}}\frac{2\tau^{2}}{\Omega^{2}}\\
\end{eqnarray}

\end_inset

 
\begin_inset Formula 
\begin{eqnarray}
\xi_{21} & \equiv & \frac{2T(-T\theta+2\kappa+4ix\kappa)}{8\theta\kappa}\\
 & = & \frac{2T(-T\theta+2\kappa+4ix\kappa)\left(-T\theta+2\kappa-4ix\kappa\right)}{8\theta\kappa\left(-T\theta+2\kappa-4ix\kappa\right)}\\
 & = & \frac{2T(\left(-T\theta+2\kappa\right)^{2}+16x^{2}\kappa^{2})}{8\theta\kappa\left(-T\theta+2\kappa-4ix\kappa\right)}\\
 & = & \frac{2T(2T^{2}\theta^{2}+4\kappa^{2}-4T\theta\kappa+16x^{2}\kappa^{2})}{8\theta\kappa\left(-T\theta+2\kappa-4ix\kappa\right)}\\
 & \approx & \frac{2T(4\kappa^{2}-4T\theta\kappa)}{8\theta\kappa\left(-T\theta+2\kappa-4ix\kappa\right)}\\
 & = & \frac{2T(4\kappa^{2}/\tau-4T\Delta\kappa)}{8\Delta\kappa\left(-T\theta+2\kappa-4ix\kappa\right)}\\
 & = & \frac{2T(4\kappa/\tau-4T\Delta)}{8\Delta\left(-T\theta+2\kappa-4ix\kappa\right)}\\
 & = & \frac{2T(4\frac{T\tau}{4\Omega^{2}}\iota_{c}/\tau-4T\Delta)}{8\Delta\left(-T\theta+2\frac{T\tau}{4\Omega^{2}}\iota_{c}-4ix\frac{T\tau}{4\Omega^{2}}\iota_{c}\right)}\\
 & = & \frac{2T(\frac{T\tau}{4\Omega^{2}}\iota_{c}/\tau^{2}-\gamma\Delta)}{2\Delta\left(-T\theta+2\frac{T\tau}{4\Omega^{2}}\iota_{c}-4ix\frac{T\tau}{4\Omega^{2}}\iota_{c}\right)/\tau}\\
 & = & \frac{T(\frac{T}{4\tau\Omega^{2}}\iota_{c}-\gamma\Delta)}{\Delta\left(-T\Delta+\frac{T}{2\Omega^{2}}\iota_{c}-ix\frac{T}{\Omega^{2}}\iota_{c}\right)}\\
 & = & \frac{T(\frac{T}{4\tau}\iota_{c}-\gamma\Delta\Omega^{2})}{\Delta\left(-T\Delta\Omega^{2}+\frac{T}{2}\iota_{c}-ixT\iota_{c}\right)}\\
 & = & \frac{T(\gamma\iota_{c}-\gamma\Delta\Omega^{2})}{\Delta\left(-T\Delta\Omega^{2}+\frac{T}{2}\iota_{c}-ixT\iota_{c}\right)}\\
 & = & \frac{\gamma\iota_{c}-\gamma\Delta\Omega^{2}}{\Delta\left(-\Delta\Omega^{2}+\frac{1}{2}\iota_{c}-ix\iota_{c}\right)}\\
\end{eqnarray}

\end_inset

Also 
\begin_inset Formula 
\begin{eqnarray}
\xi_{21} & = & \frac{T\left(16(2+T)\theta+\frac{T(T-4ix)^{2}}{\kappa}\right)}{64\theta^{2}}\\
 & = & \frac{T\left(16(2+T)\theta\kappa+T^{3}-4ixT^{2}-16Tx^{2}\right)}{64\theta^{2}\kappa}\\
 & = & \frac{T\left(16(2+T)\theta\kappa+T^{3}-4ixT^{2}-16Tx^{2}\right)}{64\theta^{2}\kappa\left(16(2+T)\theta\kappa+T^{3}+4ixT^{2}-16Tx^{2}\right)}\\
\end{eqnarray}

\end_inset


\end_layout

